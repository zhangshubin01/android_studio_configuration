<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Commit...">
        <Item>
          <IdeaActionID>CheckinProject</IdeaActionID>
          <ShortCut>'⌘K'</ShortCut>
          <Description>Commit...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B' or '⌘Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>108</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find">
        <Item>
          <IdeaActionID>ActivateFindToolWindow</IdeaActionID>
          <ShortCut>'⌘3'</ShortCut>
          <Description>Find</Description>
          <Count>8</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find Usages">
        <Item>
          <IdeaActionID>FindUsages</IdeaActionID>
          <ShortCut>'⌥F7'</ShortCut>
          <Description>Find Usages</Description>
          <Count>21</Count>
        </Item>
      </Statistic>
      <Statistic Action="Generate...">
        <Item>
          <IdeaActionID>Generate</IdeaActionID>
          <ShortCut>'⌘N' or '⌃⏎'</ShortCut>
          <Description>Generate...</Description>
          <Count>7</Count>
        </Item>
      </Statistic>
      <Statistic Action="Logcat">
        <Item>
          <IdeaActionID>ActivateLogcatToolWindow</IdeaActionID>
          <ShortCut>'⌘6'</ShortCut>
          <Description>Logcat</Description>
          <Count>1</Count>
