        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'⌘1'</ShortCut>
          <Description>Project</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Push...">
        <Item>
          <IdeaActionID>Vcs.Push</IdeaActionID>
          <ShortCut>'⇧⌘K'</ShortCut>
          <Description>Push...</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rename...">
        <Item>
          <IdeaActionID>RenameElement</IdeaActionID>
          <ShortCut>'⇧F6'</ShortCut>
          <Description>Rename...</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'app'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'⌃R'</ShortCut>
          <Description>Run 'app'</Description>
